{
  "name": "sensor-pilot",
  "version": "1.0.0",
  "description": "A progressive web app (PWA) for interacting with Bluetooth LE sensors in the browser",
  "author": "Kaspars Dambis <kaspars@kaspars.net>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/kasparsd/sensor-pilot.git"
  },
  "scripts": {
    "start": "webpack serve --mode development --open --hot --config webpack.config.js",
    "dev": "webpack --mode development --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "clean": "rimraf public",
    "serve": "serve -s public -l 5000",
    "lint": "eslint 'src/**'",
    "lint:fix": "eslint --fix 'src/**'",
    "format": "prettier --write .",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "prop-types": "^15.8.1",
    "@babel/polyfill": "^7.12.1",
    "core-js": "^3.30.0",
    "regenerator-runtime": "^0.13.11",
    "bootstrap": "^5.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.8.1",
    "sass": "^1.74.1",
    "sass-loader": "^13.2.1",
    "mini-css-extract-plugin": "^2.8.0",
    "html-webpack-plugin": "^5.5.1",
    "workbox-webpack-plugin": "^6.5.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.0",
    "eslint": "^8.48.0",
    "standard": "^17.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "serve": "^14.2.0",
    "cross-env": "^7.0.3"
  },
  "keywords": [
    "pwa",
    "bluetooth",
    "web-bluetooth",
    "react",
    "sensors"
  ],
  "engines": {
    "node": ">=14"
  }
}
